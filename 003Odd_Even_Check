#include<bits/stdc++.h>
using namespace std;

bool isodd_even(int n)
{
    // int lsb=(n&1)==0 ? 0: 1;
    // lsb==1 non-zero value means true
    // lsb==0 means false
    return lsb;
}

int main()
{
    int n=3;
    if(isodd_even(n))
    {
        cout<<n<<" is odd "<<endl;
    }
    else cout<<n<<" is even "<<endl;
    return 0;
}
